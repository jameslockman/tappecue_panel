<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtensionManifest Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.jameslockman.cep.tappecue" ExtensionBundleVersion="1.4.4" ><!-- MAJOR-VERSION-UPDATE-MARKER -->
    <ExtensionList>
        <Extension Id="com.jameslockman.cep.tappecue.panel" Version="1.4.4"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="DRWV" Version="16.0"/>
            <Host Name="FLPR" Version="14.0"/>
            <Host Name="IDSN" Version="11.0"/>
            <Host Name="AICY" Version="11.0"/>
            <Host Name="ILST" Version="18.0"/>
            <Host Name="PHSP" Version="14.0"/>
            <Host Name="PHXS" Version="14.0"/>
            <Host Name="PPRO" Version="9.0"/>
            <Host Name="PRLD" Version="5.0"/>
            <Host Name="AEFT" Version="13.0"/>
            <Host Name="AUDT" Version="9.0"/>
            <!-- <Host Name="DEMO" Version="1.0"/> -->
            <Host Name="KBRG" Version="7.0"/>
            <!-- Entry below required to support Add-ons Marketplace and Bridge -->
            <!-- <Host Name="BRDG" Version="7.0"/> -->
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="8.0"/>
            <!-- MAJOR-VERSION-UPDATE-MARKER -->
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.jameslockman.cep.tappecue.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./html/index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-file-cookies</Parameter>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Tappecue</Menu>
                    <Geometry>
                        <Size>
                            <Height>450</Height>
                            <Width>300</Width>
                        </Size>
                        <MaxSize>
                            <Height>1200</Height>
                            <Width>800</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>450</Height>
                            <Width>300</Width>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./img/TappecueIconLight.png</Icon>
                        <Icon Type="RollOver">./img/TappecueIconLight.png</Icon>
                        <Icon Type="DarkNormal">./img/TappecueIconDark.png</Icon>
                        <Icon Type="DarkRollover">./img/TappecueIconDark.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
            <DispatchInfo Host="KBRG">
                <Resources>
                    <MainPath>./html/index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-file-cookies</Parameter>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Embedded</Type>
                    <Menu>Tappecue</Menu>
                    <Geometry>
                        <Size>
                            <Height>450</Height>
                            <Width>300</Width>
                        </Size>
                        <MaxSize>
                            <Height>1200</Height>
                            <Width>800</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>450</Height>
                            <Width>300</Width>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./img/TappecueIconLight.png</Icon>
                        <Icon Type="RollOver">./img/TappecueIconLight.png</Icon>
                        <Icon Type="DarkNormal">./img/TappecueIconDark.png</Icon>
                        <Icon Type="DarkRollover">./img/TappecueIconDark.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
